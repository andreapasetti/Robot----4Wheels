<?xml version='1.0'?>

 <robot name="my_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

<!-- Color Definition -->

  <gazebo reference="chassis">
    <material>Gazebo/Orange</material>
  </gazebo>
  <gazebo reference="front_left_wheel">
    <material>Gazebo/Black</material>
  </gazebo>
  <gazebo reference="front_right_wheel">
    <material>Gazebo/Orange</material>
  </gazebo>
  <gazebo reference="back_left_wheel">
    <material>Gazebo/Black</material>
  </gazebo>
  <gazebo reference="back_right_wheel">
    <material>Gazebo/Black</material>
  </gazebo>
  <gazebo reference="lidar_base">
    <material>Gazebo/White</material>
  </gazebo>
  <gazebo reference="camera">
    <material>Gazebo/Black</material>
  </gazebo>

<!-- Robot Footprint -->

  <link name="robot_footprint"></link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

<!-- Robot Chassis -->

  <link name='chassis'>

    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="15.0"/>
      <origin xyz="0 0 0.1" rpy=" 0 0 0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>

    <visual name='chassis_visual'>
      <origin xyz="0 0 0.1" rpy=" 0 0 0"/>
      <geometry>
        <box size=".5 .3 .05"/>
      </geometry>
    </visual>

    <collision name='collision'>
      <origin xyz="0 0 0.1" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".5 .3 .05"/>
      </geometry>
    </collision>

   </link>

<!-- Robot Front Left Wheel Link-->

   <link name='front_left_wheel'>

    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0" rpy=" 0 1.5705 1.5705"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    
    <visual name='front_left_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5705 1.5705"/>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </visual>

    <collision name='front_left_collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5705 1.5705"/> 
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </collision>

   </link>

<!-- Robot Front Left Wheel Joint-->

   <joint name="front_left_hinge" type="revolute">
     <origin xyz="0.15 0.2 0.1" rpy="0 0 0"/>
     <parent link="chassis"/>
     <child link="front_left_wheel"/>
     <axis xyz="0 1 0"/>
     <limit effort="10000" velocity="1000"/>
     <dynamics damping="1.0" friction="1.0"/>
   </joint>

<!-- Robot Front Right Wheel Link-->

   <link name='front_right_wheel'>

    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0" rpy=" 0 1.5705 1.5705"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    
    <visual name='front_right_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5705 1.5705 "/>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </visual>

    <collision name='front_right_collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5705 1.5705"/> 
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </collision>

   </link>

<!-- Robot Front Right Wheel Joint-->

   <joint name="front_right_hinge" type="revolute">
     <origin xyz="0.15 -0.2 0.1" rpy="0 0 0"/>
     <parent link="chassis"/>
     <child link="front_right_wheel"/>
     <axis xyz="0 1 0"/>
     <limit effort="10000" velocity="1000"/>
     <dynamics damping="1.0" friction="1.0"/>
   </joint>

<!-- Robot Back Left Wheel Link-->

   <link name='back_left_wheel'>

    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0" rpy=" 0 1.5705 1.5705"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    
    <visual name='back_left_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5705 1.5705"/>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </visual>

    <collision name='back_left_collision'>
      <origin xyz="0 0 0 " rpy=" 0 1.5705 1.5705"/> 
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </collision>

   </link>

<!-- Robot Back Left Wheel Joint-->

   <joint name="back_left_hinge" type="revolute">
     <origin xyz="-0.15 0.2 0.1" rpy="0 0 0"/>
     <parent link="chassis"/>
     <child link="back_left_wheel"/>
     <axis xyz="0 1 0"/>
     <limit effort="10000" velocity="1000"/>
     <dynamics damping="1.0" friction="1.0"/>
   </joint>

<!-- Robot Back Right Wheel Link-->

   <link name='back_right_wheel'>

    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0" rpy=" 1.5705 0 0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    
    <visual name='back_right_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5705 1.5705"/>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </visual>

    <collision name='back_right_collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5705 1.5705"/> 
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </collision>

   </link>

<!-- Robot Back Right Wheel Joint-->

   <joint name="back_right_hinge" type="revolute">
     <origin xyz="-0.15 -0.2 0.1" rpy="0 0 0"/>
     <parent link="chassis"/>
     <child link="back_right_wheel"/>
     <axis xyz="0 1 0"/>
     <limit effort="10000" velocity="1000"/>
     <dynamics damping="1.0" friction="1.0"/>
   </joint>

<!-- Lidar Base Link-->

   <link name='lidar_base'>

    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    
    <visual name='lidar_base_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </visual>

   <collision name='lidar_base_collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </collision>

   </link>

<!-- Lidar Base Joint-->

   <joint name="lidar_link" type="fixed">
     <origin xyz="0 0 0.175" rpy="0 0 0"/>
     <parent link="chassis"/>
     <child link="lidar_base"/>
     <axis xyz="0 0 1"/>
   </joint>

<!-- Camera Link-->

   <link name='camera'>

    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6"/>
    </inertial>
    
    <visual name='camera_visual'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size=".025 .05 .05"/>
      </geometry>
    </visual>

    <collision name='camera_collision'>
      <origin xyz="0 0 0" rpy="0 0 0"/> 
      <geometry>
        <box size=".025 .05 .05"/>
      </geometry>
    </collision>

   </link>

<!-- Camera Joint-->

   <joint name="camera_link" type="fixed">
     <origin xyz="0.25 0 0.1" rpy="0 0 0"/>
     <parent link="chassis"/>
     <child link="camera"/>
     <axis xyz="0 1 0"/>
   </joint>

 </robot>

