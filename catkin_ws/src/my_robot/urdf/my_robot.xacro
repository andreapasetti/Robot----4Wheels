<?xml version='1.0'?>

 <robot name="my_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <link name="robot_footprint"></link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

  <link name='chassis'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="15.0"/>
      <origin xyz="0 0 0.1" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <visual name='chassis_visual'>
      <origin xyz="0 0 0.1" rpy=" 0 0 0"/>
      <geometry>
        <box size=".5 .3 .05"/>
      </geometry>
    </visual>

    <collision name='collision'>
      <origin xyz="0 0 0.1" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".5 .3 .05"/>
      </geometry>
    </collision>
   </link>

   <link name='front_left_wheel'>
    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0" rpy=" 0 1.5705 1.5705"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    
    <visual name='front_left_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5705 1.5705"/>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </visual>

    <collision name='front_left_collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5705 1.5705"/> 
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </collision>
   </link>

   <joint name="front_left_hinge" type="revolute">
     <origin xyz="0.15 0.2 0.1" rpy="0 0 0"/>
     <parent link="chassis"/>
     <child link="front_left_wheel"/>
     <axis xyz="0 1 0"/>
     <limit effort="10000" velocity="1000"/>
     <dynamics damping="1.0" friction="1.0"/>
   </joint>

   <link name='front_right_wheel'>
    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0" rpy=" 0 1.5705 1.5705"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    
    <visual name='front_right_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5705 1.5705 "/>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </visual>

    <collision name='front_right_collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5705 1.5705"/> 
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </collision>
   </link>

   <joint name="front_right_hinge" type="revolute">
     <origin xyz="0.15 -0.2 0.1" rpy="0 0 0"/>
     <parent link="chassis"/>
     <child link="front_right_wheel"/>
     <axis xyz="0 1 0"/>
     <limit effort="10000" velocity="1000"/>
     <dynamics damping="1.0" friction="1.0"/>
   </joint>

   <link name='back_left_wheel'>
    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0" rpy=" 0 1.5705 1.5705"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    
    <visual name='back_left_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5705 1.5705"/>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </visual>

    <collision name='back_left_collision'>
      <origin xyz="0 0 0 " rpy=" 0 1.5705 1.5705"/> 
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </collision>
   </link>

   <joint name="back_left_hinge" type="revolute">
     <origin xyz="-0.15 0.2 0.1" rpy="0 0 0"/>
     <parent link="chassis"/>
     <child link="back_left_wheel"/>
     <axis xyz="0 1 0"/>
     <limit effort="10000" velocity="1000"/>
     <dynamics damping="1.0" friction="1.0"/>
   </joint>

   <link name='back_right_wheel'>
    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0" rpy=" 1.5705 0 0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    
    <visual name='back_right_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5705 1.5705"/>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </visual>

    <collision name='back_right_collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5705 1.5705"/> 
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </collision>
   </link>

   <joint name="back_right_hinge" type="revolute">
     <origin xyz="-0.15 -0.2 0.1" rpy="0 0 0"/>
     <parent link="chassis"/>
     <child link="back_right_wheel"/>
     <axis xyz="0 1 0"/>
     <limit effort="10000" velocity="1000"/>
     <dynamics damping="1.0" friction="1.0"/>
   </joint>

   <link name='lidar_base'>
    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    
    <visual name='lidar_base_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </visual>

   <collision name='lidar_base_collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </collision>
   </link>

   <joint name="lidar_link" type="fixed">
     <origin xyz="0 0 0.175" rpy="0 0 0"/>
     <parent link="chassis"/>
     <child link="lidar_base"/>
     <axis xyz="0 0 1"/>
   </joint>

 </robot>

